{
  "name": "@homelab/playwright-server",
  "version": "1.0.0",
  "description": "Playwright test report aggregation and viewing server",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {

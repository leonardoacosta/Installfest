    "@trpc/server": "^11.0.0-rc.364",
    "hono": "^4.0.0",
    "chokidar": "^3.5.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}

{
  "additional-repositories": ["multilib"],
  "archinstall-language": "English",
  "audio_config": {
    "audio": "pipewire"
  },
  "bootloader": "systemd-bootctl",
  "config_version": "2.8.3",
  "debug": false,
  "disk_config": {
    "config_type": "default_layout",
    "device_modifications": [
      {
        "device": "/dev/sda",
        "partitions": [
          {
            "btrfs": [],
            "dev_path": null,
            "flags": ["Boot"],
            "fs_type": "fat32",
            "mount_options": [],
            "mountpoint": "/boot",
            "obj_id": "efi_partition",
            "size": {"sector_size": null, "total_size": null, "unit": "GiB", "value": 1},
            "start": {"sector_size": null, "total_size": null, "unit": "MiB", "value": 1},
            "status": "create",
            "type": "primary"
          },
          {
            "btrfs": [],
            "dev_path": null,
            "flags": [],
            "fs_type": "swap",
            "mount_options": [],
            "mountpoint": null,
            "obj_id": "swap_partition",
            "size": {"sector_size": null, "total_size": null, "unit": "GiB", "value": 8},
            "start": null,
            "status": "create",
            "type": "primary"
          },
          {
            "btrfs": [],
            "dev_path": null,
            "flags": [],
            "fs_type": "ext4",
            "mount_options": [],
            "mountpoint": "/",
            "obj_id": "root_partition",
            "size": {"sector_size": null, "total_size": null, "unit": "B", "value": 0},
            "start": null,
            "status": "create",
            "type": "primary"
          }
        ],
        "wipe": true
      }
    ]
  },
  "hostname": "homelab",
  "kernels": ["linux"],
  "locale_config": {
    "kb_layout": "us",
    "sys_enc": "UTF-8",
    "sys_lang": "en_US"
  },
  "mirror_config": {
    "mirror_regions": {"United States": ["https://geo.mirror.pkgbuild.com/$repo/os/$arch"]}
  },
  "network_config": {
    "type": "nm"
  },
  "no_pkg_lookups": false,
  "ntp": true,
  "offline": false,
  "packages": [
    "docker",
    "docker-compose",
    "git",
    "curl",
    "wget",
    "vim",
    "htop",
    "btop",
    "tmux",
    "rsync",
    "bluez",
    "bluez-utils",
    "steam",
    "base-devel",
    "openssh",
    "networkmanager",
    "gnupg",
    "yq"
  ],
  "parallel downloads": 5,
  "profile_config": {
    "gfx_driver": "All open-source (default)",
    "greeter": null,
    "profile": {
      "details": [],
      "main": "Minimal"
    }
  },
  "script": "guided",
  "silent": false,
  "swap": true,
  "timezone": "America/Chicago",
  "version": "2.8.3"
}
